{
    "name": "nwallet",
    "version": "0.0.1",
    "description": "Ncoin-NWallet project",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://https://github.com/ncoin/nwallet.git"
    },
    "scripts": {
        "build": "ionic build",
        "serve": "ionic serve",
        "start": "npm run serve",
        "env:dev": "node ./src/environments/env_apply.js src/environments/environment.dev.ts src/environments/environment.ts \n\n# Environment set to: dev\n\n",
        "env:stage": "node ./src/environments/env_apply.js src/environments/environment.stage.ts src/environments/environment.ts \n\n# Environment set to: e2e\n\n",
        "env:prod": "node ./src/environments/env_apply.js src/environments/environment.prod.ts src/environments/environment.ts \n\n# Environment set to: prod\n\n",
        "network:test": "node ./src/environments/env_apply.js src/environments/schema.test.ts src/environments/schema.ts \n\n# Network set to: test network\n\n",
        "network:public": "node ./src/environments/env_apply.js src/environments/schema.public.ts src/environments/schema.ts \n\n# Network set to: public network\n\n",
        "build:prod": "ionic cordova build android --prod --release",
        "start:debug": "ionic cordova run android --device"
    },
    "dependencies": {
        "@angular/common": "5.0.0",
        "@angular/compiler": "5.0.0",
        "@angular/compiler-cli": "5.0.0",
        "@angular/core": "5.0.0",
        "@angular/forms": "5.0.0",
        "@angular/http": "5.0.0",
        "@angular/platform-browser": "5.0.0",
        "@angular/platform-browser-dynamic": "5.0.0",
        "@ionic-native/clipboard": "^4.12.2",
        "@ionic-native/core": "4.10.1",
        "@ionic-native/device": "4.10.1",
        "@ionic-native/fingerprint-aio": "4.10.1",
        "@ionic-native/in-app-browser": "4.10.1",
        "@ionic-native/qr-scanner": "^4.12.2",
        "@ionic-native/secure-storage": "4.10.1",
        "@ionic-native/splash-screen": "4.10.1",
        "@ionic-native/status-bar": "4.10.1",
        "@ionic-native/vibration": "4.10.1",
        "@ionic/pro": "1.0.20",
        "@ionic/storage": "2.1.3",
        "@ngx-translate/core": "^9.1.1",
        "@ngx-translate/http-loader": "^2.0.0",
        "cordova-android": "^7.1.1",
        "cordova-android-support-gradle-release": "^1.4.4",
        "cordova-clipboard": "^1.2.1",
        "cordova-ios": "git+https://github.com/jrryhrtn/cordova-ios.git",
        "cordova-plugin-device": "^1.1.6",
        "cordova-plugin-fingerprint-aio": "^1.3.8",
        "cordova-plugin-inappbrowser": "^1.7.1",
        "cordova-plugin-ionic-keyboard": "^2.0.2",
        "cordova-plugin-ionic-webview": "^1.1.15",
        "cordova-plugin-qrscanner": "^2.6.0",
        "cordova-plugin-secure-storage": "^2.6.8",
        "cordova-plugin-splashscreen": "^4.0.3",
        "cordova-plugin-statusbar": "^2.2.3",
        "cordova-plugin-vibration": "^3.1.0",
        "cordova-plugin-whitelist": "^1.3.3",
        "crc": "^3.5.0",
        "font-awesome": "^4.7.0",
        "forge": "git+https://github.com/hnoh/forge.git",
        "ionic-angular": "3.9.2",
        "ionicons": "3.0.0",
        "js-xdr": "^1.0.3",
        "ngx-qrcode2": "0.0.9",
        "rxjs": "5.5.2",
        "stellar-base": "^0.7.8",
        "stellar-sdk": "^0.8.2",
        "sw-toolbox": "3.6.0",
        "zone.js": "0.8.18"
    },
    "devDependencies": {
        "@ionic/app-scripts": "3.1.11",
        "@types/lodash": "^4.14.115",
        "@types/stellar-sdk": "^0.8.4",
        "codelyzer": "^4.4.2",
        "tslint": "^5.10.0",
        "typescript": "^2.4.2"
    },
    "config": {
        "ionic_aot_write_to_disk": true,
        "ionic_source_map": "source_map",
        "ionic_source_map_type": "source-map",
        "//ionic_webpack": "./config/webpack.config.js",
        "ionic_copy": "./config/copy.config.js",
        "ionic_sass": "./config/sass.config.js"
    },
    "cordova": {
        "platforms": [
            "android",
            "ios"
        ],
        "plugins": {
            "cordova-plugin-device": {},
            "cordova-plugin-inappbrowser": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-ionic-webview": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-ionic-keyboard": {},
            "cordova-plugin-secure-storage": {},
            "cordova-android-support-gradle-release": {
                "ANDROID_SUPPORT_VERSION": "27.+"
            },
            "cordova-plugin-vibration": {},
            "cordova-plugin-whitelist": {},
            "cordova-plugin-fingerprint-aio": {},
            "cordova-plugin-qrscanner": {},
            "cordova-clipboard": {}
        }
    }
}
